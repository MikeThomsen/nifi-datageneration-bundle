                            <key>article</key>
                            <value>&lt;html&gt;
&lt;body&gt;
	&lt;p&gt;{{header}}&lt;/p&gt;
	&lt;table&gt;
		&lt;thead&gt;
			&lt;tr&gt;
				&lt;th&gt;First Name&lt;/th&gt;
				&lt;th&gt;Last Name&lt;/th&gt;
				&lt;th&gt;Department&lt;/th&gt;
				&lt;th&gt;Years of Service&lt;/th&gt;
			&lt;/tr&gt;
		&lt;/thead&gt;
		&lt;tbody&gt;
		{% for user in users %}
			&lt;tr&gt;
				&lt;td&gt;{{user.firstName}}&lt;/td&gt;
				&lt;td&gt;{{user.lastName}}&lt;/td&gt;
				&lt;td&gt;{{user.department}}&lt;/td&gt;
				&lt;td&gt;{{user.yearsOfService}}&lt;/td&gt;
			&lt;/tr&gt;
		{% endfor %}
		&lt;/tbody&gt;
	&lt;/table&gt;
	&lt;blockquote&gt;{{summary}}&lt;/blockquote&gt;
	{% for para in body %}
	&lt;p&gt;{{para}}&lt;/p&gt;
	{% endfor %}
&lt;/body&gt;
&lt;/html&gt;</value>
